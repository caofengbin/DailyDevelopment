<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/activity_liife_cycle"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cfb.com.dailydevelopment.example3.lifecycle.LifeCycleActivity">

    <Button
        android:id="@+id/start_activity1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="启动Normal Activity"/>

    <Button
        android:id="@+id/start_activity2"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:text="启动Dialog Activity"/>
</LinearLayout>
